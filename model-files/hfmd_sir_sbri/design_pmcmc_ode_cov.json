{
  "name": "pmcmc_ode_cov",
  "description": "get a cov matrix pmcmc ode",
  "id": "replicate",
  "H": 5,
  "seed": "it will converge",
  "cmd": [
    {
      "pipe": "-u 0.01",
      "algorithm": "pmcmc ode -J 1 -M 100000 --full --acc --smooth -S 10000000 --quiet"
    },
    {
      "pipe": "-C -T -u 0.01",
      "algorithm": "pmcmc ode -J 1 -M 100000 --full --acc --smooth -S 10000000 --quiet",
      "repeat": 4
    }
  ]
}