{
  "name": "kmcmc_cov",
  "description": "get a cov matrix kmcmc full update",
  "id": "replicate",
  "H": 5,
  "seed": "it will converge",
  "cmd": [
    {
      "pipe": "-u 0.01",
      "algorithm": "kmcmc -M 100000 --full --no_dem_sto --acc --smooth -S 10000000 --quiet"
    },
    {
      "pipe": "-C -T -u 0.01",
      "algorithm": "kmcmc -M 100000 --full --no_dem_sto --acc --smooth -S 10000000 --quiet",
      "repeat": 4
    }
  ]
}